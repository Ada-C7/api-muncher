<section class="recipe-list">

<h2>Results for your search!</h2>
<div class="row">
<div class="medium-4 medium-centered columns">
  <div class="transbox">
    <div id="recipes">

  <ul>
    <% @recipes.each do | recipe | %>
      <li>
      <%=link_to recipe.label, ingredients_path + "?#{recipe.uri}" %>
      <%=recipe.image %>

      </li>
    <% end %>
  </ul>
    </div>
  </div>
</section>
