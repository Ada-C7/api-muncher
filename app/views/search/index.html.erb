
<p>
<%@searches.each do |key, recipe|%>
<%=image_tag src="#{recipe["image"]}"%>

<% uri = @search_object[key.to_i]["uri"]%>
<% from = @search_object[key.to_i]["from"]%>
<%=link_to recipe["label"], search_path( key, "uri" => recipe["uri"], "from"=> from) %>
<%end%>

<%from = @search.from.to_i + 10%>
<% query = params[:query]%>
<%=link_to ">>", searches_path( "query"=> query, "from"=> from) %>
