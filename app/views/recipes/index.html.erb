<h1>Recipes#index</h1>
<p>Hello</p>

<%= form_tag(recipes_path, method: :get) do %>
<%= label_tag(:q, "Search for:") %>
<%= text_field_tag(:q) %>
<%= submit_tag("Search") %>
<% end %>

<ul>
<% @recipes.each do |recipe| %>
  <li>
    <%= recipe.label %><%= image_tag(recipe.image) %><%= recipe.dietery_info %>
  </li>
<% end  %>
</ul>
